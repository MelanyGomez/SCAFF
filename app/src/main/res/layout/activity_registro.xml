<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".pantallas.Registro_act">
    <Button
        android:id="@+id/materialButton2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:backgroundTint="@color/black1"
        android:text="MATERIAL"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:layout_marginStart="0dp"
        app:cornerRadius="0dp"
        app:layout_constraintEnd_toStartOf="@+id/maquinariaButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <!--button7-->
    <Button
        android:id="@+id/maquinariaButton2"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:backgroundTint="@color/black1"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:layout_marginStart="130dp"
        android:text="MAQUINAS"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:cornerRadius="0dp"
        app:layout_constraintTop_toTopOf="parent" />
    <!--button8-->
    <Button
        android:id="@+id/registroButton2"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:backgroundTint="@color/black1"
        android:layout_marginStart="280dp"
        android:text="REGISTRO"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:cornerRadius="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</RelativeLayout>